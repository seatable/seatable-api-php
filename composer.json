{
  "name": "seatable/seatable-api-php",
  "description": "Simple PHP class to use the SeaTable Web API",
  "type": "library",
  "keywords": ["SeaTable"],
  "homepage": "https://github.com/seatable/seatable-api-php",
  "readme": "README.md",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Dyllick-Brenzinger"
    }
  ],
  "support": {
    "forum": "https://forum.seatable.io/",
    "source": "https://github.com/seatable/seatable-api-php"
  },
  "require": {
    "php": ">=7.0",
    "ext-curl": "*",
    "ext-json": "*"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "^3.0.0",
    "kubawerlos/php-cs-fixer-custom-fixers": "^2.5"
  },
  "autoload": {
    "classmap": ["SeaTableAPI.php"]
  },
  "scripts": {
    "test": [
      "@composer --no-plugins --version",
      "@composer --no-plugins validate --no-check-lock --strict",
      "@composer --no-plugins dump-autoload",
      "test/autoload.php",
      "@composer --no-plugins update --no-scripts --no-progress",
      "@composer --no-plugins outdated --direct --strict",
      "@composer --no-plugins exec -- php-cs-fixer fix ${CI:+--dry-run} --diff"
    ]
  },
  "scripts-descriptions": {
    "test": "run all tests"
  },
  "config": {
    "sort-packages": true
  }
}

{
  "name": "seatable/seatable-api-php",
  "description": "The official SeaTable API Reference (OpenAPI 3.0).",
  "type": "library",
  "keywords": [
    "openapitools",
    "openapi-generator",
    "openapi",
    "php",
    "sdk",
    "rest",
    "api",
    "library"
  ],
  "homepage": "https://api.seatable.io/",
  "readme": "README.md",
  "license": "Apache-2.0",
  "authors": [
    {
      "name": "Christoph Dyllick-Brenzinger",
      "homepage": "https://seatable.io"
    },
    {
      "name": "OpenAPI",
      "homepage": "https://openapi-generator.tech"
    }
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "ext-curl": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "guzzlehttp/guzzle": "^7.3",
    "guzzlehttp/psr7": "^1.7 || ^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0 || ^9.0",
    "friendsofphp/php-cs-fixer": "^3.5"
  },
  "autoload": {
    "psr-4": {
      "SeaTable\\Client\\": "lib/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SeaTable\\Client\\Test\\": "test/"
    }
  }
}
